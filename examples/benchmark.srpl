main {
	var start_time = timer!();

	#################
	# Program Start #
	#################

	var mut i = 0;
	var num = 10000000;
	while (i < num)
		mut i += 1;

	#################
	#  Program End  #
	#################

	var end_time = timer!();
	println!("program took " <> as_str!(end_time - start_time) <> "s to complete");
}
